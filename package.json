    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-zoom": "^3.0.0",
    "eslint": "^8.4.1",
    "moment": "^2.29.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-resizable": "^3.0.4",
